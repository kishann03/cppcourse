Q5-
#include<bits/stdc++.h>
using namespace std;
int main(){
    int jump;
    string s;
    cout<<"enter the value of jump:\n";
    cin>>jump;
    if(jump<0 || jump>=6){
        cout<<"invalid value for jump\n";
        return 0;
    }
    cout<<"enter the string:\n";
    cin>>s;
    if(s.length()<jump){
        cout<<-1;
        return 0;
    }
    for( int i = 0; i<s.length();i+=jump){
        cout<<s[i];
    }
}

